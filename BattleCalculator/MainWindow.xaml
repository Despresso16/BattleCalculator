<Window x:Class="BattleCalculator.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BattleCalculator"
        mc:Ignorable="d"
        Title="Battle Calculator" Height="500" Width="800">
    <Grid>
        <Grid x:Name="pageGrid">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="1*"/>
            </Grid.ColumnDefinitions>
            <Grid.RowDefinitions>
                <RowDefinition Height="1*"/>
            </Grid.RowDefinitions>
            <Grid Margin="10" x:Name="mainPage" Grid.Column="0" Grid.Row="0">
                <Grid.ColumnDefinitions>
                    <ColumnDefinition Width="1*"/>
                    <ColumnDefinition Width="0.5*"/>
                    <ColumnDefinition Width="1*"/>
                </Grid.ColumnDefinitions>
                <Grid.RowDefinitions>
                    <RowDefinition Height="1*"/>
                    <RowDefinition Height="2*"/>
                </Grid.RowDefinitions>
                <GroupBox Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Header="Ustawienia" Margin="5">
                    <Grid>
                        <Grid.ColumnDefinitions>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                            <ColumnDefinition Width="1*"/>
                        </Grid.ColumnDefinitions>
                        <Grid.RowDefinitions>
                            <RowDefinition Height="1*"/>
                            <RowDefinition Height="1*"/>
                        </Grid.RowDefinitions>
                        <ComboBox Margin="15" Grid.Column="0" Grid.Row="0" x:Name="cbBattleType" VerticalContentAlignment="Center" LostFocus="cbBattleType_LostFocus">
                            <ComboBoxItem Content="Wybierz typ bitwy" IsSelected="True" IsEnabled="False" />
                            <ComboBoxItem Content="Bitwa lądowa"/>
                            <ComboBoxItem Content="Bitwa morska"/>
                        </ComboBox>
                        <ComboBox Margin="15" Grid.Column="0" Grid.Row="1" x:Name="cbSkirmishVariant" VerticalContentAlignment="Center" LostFocus="cbSkirmishVariant_LostFocus">
                            <ComboBoxItem Content="Wybierz rodzaj spotkania" IsSelected="True" IsEnabled="False" />
                            <ComboBoxItem Content="Atak"/>
                            <ComboBoxItem Content="Spotkanie"/>
                        </ComboBox>
                        <Border Grid.Row="0" Grid.Column="1" Margin="15" BorderBrush="Gray" BorderThickness="0.5">
                            <Grid IsEnabled="False" x:Name="gridFortLevel">
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="0.2*"/>
                                    <ColumnDefinition Width="1*"/>
                                    <ColumnDefinition Width="0.2*"/>
                                </Grid.ColumnDefinitions>
                                <Button Content="-" Grid.Column="0" FontSize="15" x:Name="btnFortLevelMinus" Click="btnFortLevelMinus_Click"/>
                                <TextBlock Grid.Column="1" Text="Poziom fortu: 0" FontSize="15" x:Name="tbFortLevel" HorizontalAlignment="Center" VerticalAlignment="Center"/>
                                <Button Content="+" Grid.Column="2" FontSize="15" x:Name="btnFortLevelPlus" Click="btnFortLevelPlus_Click"/>
                            </Grid>
                        </Border>
                        <!--Rodzaje terenu-->
                        <ComboBox Margin="15" Grid.Column="1" Grid.Row="1" x:Name="cbTerrainType" Visibility="Hidden" VerticalContentAlignment="Center" LostFocus="cbTerrainType_LostFocus">
                            <ComboBoxItem Content="Wybierz rodzaj terenu" IsSelected="True" IsEnabled="False"/>
                            <ComboBoxItem Content="Łąki"/>
                            <ComboBoxItem Content="Lasy"/>
                            <ComboBoxItem Content="Równiny"/>
                            <ComboBoxItem Content="Wzgórza"/>
                            <ComboBoxItem Content="Wzgórza z lasami"/>
                            <ComboBoxItem Content="Góry"/>
                            <ComboBoxItem Content="Tundra"/>
                            <ComboBoxItem Content="Bagno"/>
                            <ComboBoxItem Content="Pustynia"/>
                            <ComboBoxItem Content="Dżungla"/>
                        </ComboBox>
                        <!--Rodzaje morza-->
                        <ComboBox Margin="15" Grid.Column="1" Grid.Row="1" x:Name="cbSeaType" Visibility="Hidden" VerticalContentAlignment="Center">
                            <ComboBoxItem Content="Wybierz głębokość morza" IsSelected="True" IsEnabled="False"/>
                            <ComboBoxItem Content="Ocean"/>
                            <ComboBoxItem Content="Morze"/>
                            <ComboBoxItem Content="Płycizna"/>
                        </ComboBox>
                        <!--Placeholder za rodzaje terenu i morza-->
                        <Rectangle Margin="15" Grid.Column="1" Grid.Row="1" x:Name="cbTerrainSeaHidden" Fill="LightGray"/>
                        <Button Grid.Column="2" Grid.Row="0" Grid.RowSpan="2" Margin="30" Content="START!" FontSize="20" FontWeight="Bold" x:Name="btnStart" IsEnabled="False" Click="StartButton_Click"/>
                    </Grid>
                </GroupBox>
                <!--Wybieranie sklado armii 1-->
                <Grid Grid.Row="1" Grid.Column="0" Margin="10">
                    <Grid.ColumnDefinitions>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                        <ColumnDefinition Width="1*"/>
                    </Grid.ColumnDefinitions>
                    <Grid.RowDefinitions>
                        <RowDefinition Height="0.5*"/>
                        <RowDefinition Height="1.5*"/>
                        <RowDefinition Height="0.2*"/>
                    </Grid.RowDefinitions>
                    <StackPanel Grid.Column="0" Grid.Row="0" Grid.ColumnSpan="4">
                        <TextBlock Text="Grupa 1" HorizontalAlignment="Center" Margin="10" x:Name="txbArmy1"/>
                        <CheckBox Content="Armia spełniła specjalny warunek" x:Name="ckArmy1Condition" VerticalAlignment="Center"/>
                    </StackPanel>
                    <ScrollViewer Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="4" VerticalScrollBarVisibility="Visible" x:Name="scArmy1Selection">
                        <StackPanel Orientation="Vertical">
                            <Button Content="Jednostki startowe" x:Name="btnStartUnits1" Height="20" Click="btnStartUnits1_Click"/>
                            <ScrollViewer x:Name="scvStartUnits1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Pikinierzy" x:Name="ckArmy1Pikemen" Width="150" Click="ckArmy1Pikemen_Click"/>
                                        <TextBox x:Name="tbxArmy1Pikemen" Text="0" IsEnabled="False" LostFocus="tbxArmy1Pikemen_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Arkebuzerzy" x:Name="ckArmy1Arquebusiers" Width="150" Click="ckArmy1Arquebusiers_Click"/>
                                        <TextBox x:Name="tbxArmy1Arquebusiers" Text="0" IsEnabled="False" LostFocus="tbxArmy1Arquebusiers_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Łucznicy" x:Name="ckArmy1Archers" Width="150" Click="ckArmy1Archers_Click"/>
                                        <TextBox x:Name="tbxArmy1Archers" Text="0" IsEnabled="False" LostFocus="tbxArmy1Archers_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Kusznicy" x:Name="ckArmy1Crossbowmen" Width="150" Click="ckArmy1Crossbowmen_Click"/>
                                        <TextBox x:Name="tbxArmy1Crossbowmen" Text="0" IsEnabled="False" LostFocus="tbxArmy1Crossbowmen_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Rycerze" x:Name="ckArmy1Knights" Width="150" Click="ckArmy1Knights_Click"/>
                                        <TextBox x:Name="tbxArmy1Knights" Text="0" IsEnabled="False" LostFocus="tbxArmy1Knights_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Konnica" x:Name="ckArmy1Horsemen" Width="150" Click="ckArmy1Horsemen_Click"/>
                                        <TextBox x:Name="tbxArmy1Horsemen" Text="0" IsEnabled="False" LostFocus="tbxArmy1Horsemen_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Bombarda" x:Name="ckArmy1Bombard" Width="150" Click="ckArmy1Bombard_Click"/>
                                        <TextBox x:Name="tbxArmy1Bombard" Text="0" IsEnabled="False" LostFocus="tbxArmy1Bombard_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                            <Button Content="Epoka eksploracji" x:Name="btnExplorationEra1" Height="20" Click="btnExplorationEra1_Click"/>
                            <ScrollViewer x:Name="scvExplorationEra1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Piki i arkebuzerzy" x:Name="ckArmy1PikeShotArq" Width="150" Click="ckArmy1PikeShotArq_Click"/>
                                        <TextBox x:Name="tbxArmy1PikeShotArq" Text="0" IsEnabled="False" LostFocus="tbxArmy1PikeShotArq_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Ciężcy husarze" x:Name="ckArmy1HeavyHussars" Width="150" Click="ckArmy1HeavyHussars_Click"/>
                                        <TextBox x:Name="tbxArmy1HeavyHussars" Text="0" IsEnabled="False" LostFocus="tbxArmy1HeavyHussars_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Reiterzy" x:Name="ckArmy1Reiters" Width="150" Click="ckArmy1Reiters_Click"/>
                                        <TextBox x:Name="tbxArmy1Reiters" Text="0" IsEnabled="False" LostFocus="tbxArmy1Reiters_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Armata polowa" x:Name="ckArmy1FieldCannon" Width="150" Click="ckArmy1FieldCannon_Click"/>
                                        <TextBox x:Name="tbxArmy1FieldCannon" Text="0" IsEnabled="False" LostFocus="tbxArmy1FieldCannon_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Ciężka armata" x:Name="ckArmy1HeavyCannon" Width="150" Click="ckArmy1HeavyCannon_Click"/>
                                        <TextBox x:Name="tbxArmy1HeavyCannon" Text="0" IsEnabled="False" LostFocus="tbxArmy1HeavyCannon_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                            <Button Content="Epoka ekspansji" x:Name="btnExpansionEra1" Height="20" Click="btnExpansionEra1_Click"/>
                            <ScrollViewer x:Name="scvExpansionEra1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Piki i muszkieterzy" x:Name="ckArmy1PikeShotMusk" Width="150" Click="ckArmy1PikeShotMusk_Click"/>
                                        <TextBox x:Name="tbxArmy1PikeShotMusk" Text="0" IsEnabled="False" LostFocus="tbxArmy1PikeShotMusk_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Wcześni fusilierzy" x:Name="ckArmy1EarlyFusiliers" Width="150" Click="ckArmy1EarlyFusiliers_Click"/>
                                        <TextBox x:Name="tbxArmy1EarlyFusiliers" Text="0" IsEnabled="False" LostFocus="tbxArmy1EarlyFusiliers_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Wczesni kirasjerzy" x:Name="ckArmy1EarlyCuirassier" Width="150" Click="ckArmy1EarlyCuirassier_Click"/>
                                        <TextBox x:Name="tbxArmy1EarlyCuirassier" Text="0" IsEnabled="False" LostFocus="tbxArmy1EarlyCuirassier_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Harkebuzerzy" x:Name="ckArmy1Harquebusers" Width="150" Click="ckArmy1Harquebusers_Click"/>
                                        <TextBox x:Name="tbxArmy1Harquebusers" Text="0" IsEnabled="False" LostFocus="tbxArmy1Harquebusers_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Lansjerzy" x:Name="ckArmy1Lancers" Width="150" Click="ckArmy1Lancers_Click"/>
                                        <TextBox x:Name="tbxArmy1Lancers" Text="0" IsEnabled="False" LostFocus="tbxArmy1Lancers_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Haubica oblężnicza" x:Name="ckArmy1SiegeHowitzer" Width="150" Click="ckArmy1SiegeHowitzer_Click"/>
                                        <TextBox x:Name="tbxArmy1SiegeHowitzer" Text="0" IsEnabled="False" LostFocus="tbxArmy1SiegeHowitzer_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                            <Button Content="Epoka imperiów" x:Name="btnEmpireEra1" Height="20" Click="btnEmpireEra1_Click"/>
                            <ScrollViewer x:Name="scvEmpireEra1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Fusilierzy" x:Name="ckArmy1Fusiliers" Width="150" Click="ckArmy1Fusiliers_Click"/>
                                        <TextBox x:Name="tbxArmy1Fusiliers" Text="0" IsEnabled="False" LostFocus="tbxArmy1Fusiliers_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Grenadierzy" x:Name="ckArmy1Grenadiers" Width="150" Click="ckArmy1Grenadiers_Click"/>
                                        <TextBox x:Name="tbxArmy1Grenadiers" Text="0" IsEnabled="False" LostFocus="tbxArmy1Grenadiers_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Milicja" x:Name="ckArmy1Militia" Width="150" Click="ckArmy1Militia_Click"/>
                                        <TextBox x:Name="tbxArmy1Militia" Text="0" IsEnabled="False" LostFocus="tbxArmy1Militia_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Karbinerzy" x:Name="ckArmy1CarbineCav" Width="150" Click="ckArmy1CarbineCav_Click"/>
                                        <TextBox x:Name="tbxArmy1CarbineCav" Text="0" IsEnabled="False" LostFocus="tbxArmy1CarbineCav_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Dragoni" x:Name="ckArmy1Dragoons" Width="150" Click="ckArmy1Dragoons_Click"/>
                                        <TextBox x:Name="tbxArmy1Dragoons" Text="0" IsEnabled="False" LostFocus="tbxArmy1Dragoons_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Huzarzy" x:Name="ckArmy1Hussars" Width="150" Click="ckArmy1Hussars_Click"/>
                                        <TextBox x:Name="tbxArmy1Hussars" Text="0" IsEnabled="False" LostFocus="tbxArmy1Hussars_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Kirasjerzy" x:Name="ckArmy1Cuiraissiers" Width="150" Click="ckArmy1Cuiraissiers_Click"/>
                                        <TextBox x:Name="tbxArmy1Cuiraissiers" Text="0" IsEnabled="False" LostFocus="tbxArmy1Cuiraissiers_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Działa polowe" x:Name="ckArmy1FieldGun" Width="150" Click="ckArmy1FieldGun_Click"/>
                                        <TextBox x:Name="tbxArmy1FieldGun" Text="0" IsEnabled="False" LostFocus="tbxArmy1FieldGun_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Moździerze" x:Name="ckArmy1Mortars" Width="150" Click="ckArmy1Mortars_Click"/>
                                        <TextBox x:Name="tbxArmy1Mortars" Text="0" IsEnabled="False" LostFocus="tbxArmy1Mortars_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                            <Button Content="Epoka rewolucji" x:Name="btnRevolutionEra1" Height="20" Click="btnRevolutionEra1_Click"/>
                            <ScrollViewer x:Name="scvRevolutionEra1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Lekka piechota" x:Name="ckArmy1LightInfantry" Width="150" Click="ckArmy1LightInfantry_Click"/>
                                        <TextBox x:Name="tbxArmy1LightInfantry" Text="0" IsEnabled="False" LostFocus="tbxArmy1LightInfantry_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Piechota liniowa" x:Name="ckArmy1LineInfantry" Width="150" Click="ckArmy1LineInfantry_Click"/>
                                        <TextBox x:Name="tbxArmy1LineInfantry" Text="0" IsEnabled="False" LostFocus="tbxArmy1LineInfantry_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Haubice polowe" x:Name="ckArmy1FieldHowitzer" Width="150" Click="ckArmy1FieldHowitzer_Click"/>
                                        <TextBox x:Name="tbxArmy1FieldHowitzer" Text="0" IsEnabled="False" LostFocus="tbxArmy1FieldHowitzer_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                            <Button Content="Jednostki unikalne" x:Name="btnUniqueUnitsEra1" Height="20" Click="btnUniqueUnitsEra1_Click"/>
                            <ScrollViewer x:Name="scvUniqueUnitsEra1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Kozacy" x:Name="ckArmy1Cossacks" Width="150" Click="ckArmy1Cossacks_Click"/>
                                        <TextBox x:Name="tbxArmy1Cossacks" Text="0" IsEnabled="False" LostFocus="tbxArmy1Cossacks_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Tubylcy wojownicy" x:Name="ckArmy1TribalWarriors" Width="150" Click="ckArmy1TribalWarriors_Click"/>
                                        <TextBox x:Name="tbxArmy1TribalWarriors" Text="0" IsEnabled="False" LostFocus="tbxArmy1TribalWarriors_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Łucznicy konni" x:Name="ckArmy1HorseArcher" Width="150" Click="ckArmy1HorseArcher_Click"/>
                                        <TextBox x:Name="tbxArmy1HorseArcher" Text="0" IsEnabled="False" LostFocus="tbxArmy1HorseArcher_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Tubylcy strzelcy" x:Name="ckArmy1TribalRanger" Width="150" Click="ckArmy1TribalRanger_Click"/>
                                        <TextBox x:Name="tbxArmy1TribalRanger" Text="0" IsEnabled="False" LostFocus="tbxArmy1TribalRanger_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Konnica tubylców" x:Name="ckArmy1TribalHorsemen" Width="150" Click="ckArmy1TribalHorsemen_Click"/>
                                        <TextBox x:Name="tbxArmy1TribalHorsemen" Text="0" IsEnabled="False" LostFocus="tbxArmy1TribalHorsemen_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                        </StackPanel>
                    </ScrollViewer>
                    <ScrollViewer Grid.Row="1" Grid.Column="0" Grid.ColumnSpan="4" VerticalScrollBarVisibility="Visible" x:Name="scFleet1Selection" Visibility="Hidden">
                        <StackPanel Orientation="Vertical">
                            <Button Content="Jednostki startowe" x:Name="btnStartShips1" Height="20" Click="btnStartShips1_Click"/>
                            <ScrollViewer x:Name="scvStartShips1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Karak" x:Name="ckFleet1Carrack" Width="150" Click="ckFleet1Carrack_Click"/>
                                        <TextBox x:Name="tbxFleet1Carrack" Text="0" IsEnabled="False" LostFocus="tbxFleet1Carrack_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Karawela" x:Name="ckFleet1Caravel" Width="150" Click="ckFleet1Caravel_Click"/>
                                        <TextBox x:Name="tbxFleet1Caravel" Text="0" IsEnabled="False" LostFocus="tbxFleet1Caravel_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Galera" x:Name="ckFleet1Galley" Width="150" Click="ckFleet1Galley_Click"/>
                                        <TextBox x:Name="tbxFleet1Galley" Text="0" IsEnabled="False" LostFocus="tbxFleet1Galley_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                            <Button Content="Epoka eksploracji" x:Name="btnExplorationEraShips1" Height="20" Click="btnExplorationEraShips1_Click"/>
                            <ScrollViewer x:Name="scvExplorationEraShips1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Galeon" x:Name="ckFleet1Galleon" Width="150" Click="ckFleet1Galleon_Click"/>
                                        <TextBox x:Name="tbxFleet1Galleon" Text="0" IsEnabled="False" LostFocus="tbxFleet1Galleon_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                            <Button Content="Epoka ekspansji" x:Name="btnExpansionEraShips1" Height="20" Click="btnExpansionEraShips1_Click"/>
                            <ScrollViewer x:Name="scvExpansionEraShips1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Szkuner" x:Name="ckFleet1Schooner" Width="150" Click="ckFleet1Schooner_Click"/>
                                        <TextBox x:Name="tbxFleet1Schooner" Text="0" IsEnabled="False" LostFocus="tbxFleet1Schooner_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Bryg" x:Name="ckFleet1Brig" Width="150" Click="ckFleet1Brig_Click"/>
                                        <TextBox x:Name="tbxFleet1Brig" Text="0" IsEnabled="False" LostFocus="tbxFleet1Brig_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Fregata" x:Name="ckFleet1Frigate" Width="150" Click="ckFleet1Frigate_Click"/>
                                        <TextBox x:Name="tbxFleet1Frigate" Text="0" IsEnabled="False" LostFocus="tbxFleet1Frigate_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Ciężka fregata" x:Name="ckFleet1GreatFrigate" Width="150" Click="ckFleet1GreatFrigate_Click"/>
                                        <TextBox x:Name="tbxFleet1GreatFrigate" Text="0" IsEnabled="False" LostFocus="tbxFleet1GreatFrigate_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                            <Button Content="Epoka imperiów" x:Name="btnEmpireEraShips1" Height="20" Click="btnEmpireEraShips1_Click"/>
                            <ScrollViewer x:Name="scvEmpireEraShips1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Liniowiec" x:Name="ckFleet1ShipOfLine" Width="150" Click="ckFleet1ShipOfLine_Click"/>
                                        <TextBox x:Name="tbxFleet1ShipOfLine" Text="0" IsEnabled="False" LostFocus="tbxFleet1ShipOfLine_LostFocus"/>
                                    </StackPanel>
                                    <StackPanel Orientation="Horizontal" Height="17">
                                        <CheckBox Content="Eastindiaman" x:Name="ckFleet1Eastindiaman" Width="150" Click="ckFleet1Eastindiaman_Click"/>
                                        <TextBox x:Name="tbxFleet1Eastindiaman" Text="0" IsEnabled="False" LostFocus="tbxFleet1Eastindiaman_LostFocus"/>
                                    </StackPanel>
                                </StackPanel>
                            </ScrollViewer>
                            <Button Content="Epoka rewolucji" x:Name="btnRevolutionEraShips1" Height="20" Click="btnRevolutionEraShips1_Click"/>
                            <ScrollViewer x:Name="scvRevolutionEraShips1" VerticalScrollBarVisibility="Visible" Visibility="Collapsed">
                                <StackPanel Orientation="Vertical">
