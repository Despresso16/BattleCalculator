                        <TextBlock x:Name="tbBattleLog" FontSize="13"/>
                    </ScrollViewer>
                </StackPanel>
                <Label Grid.Row="0" Grid.Column="0" Grid.ColumnSpan="3" Content="Wynik" FontSize="50" FontFamily="Arial" HorizontalAlignment="Center"/>
                <StackPanel Grid.Row="1" Grid.Column="2" Margin="5">
                    <Border BorderBrush="Black" BorderThickness="2" >
                        <Label Content="Armia 2" FontSize="20" HorizontalAlignment="Center" x:Name="lbArmy2"/>
                    </Border>
                    <ScrollViewer VerticalScrollBarVisibility="Visible" Height="295">
                        <TextBlock x:Name="tbArmy2Results" FontSize="13"/>
                    </ScrollViewer>
                </StackPanel>
                <Button x:Name="btnResultArmy2Save" Content="Zapisz armię" Margin="5" Grid.Column="2" Grid.Row="2" FontFamily="Arial" FontSize="20"/>
            </Grid>
        </Grid>
    </Grid>
</Window>
